        "def age_calc(year,month,day):\n",
        "  import datetime\n",
        "  today = datetime.datetime.now().date()\n",
        "  day=datetime.date(year,month,day)\n",
        "  age=int((today-day).days/365.0)\n",
        "  \n",
        "  print(age)\n",
        "\n",
        "age_calc(1972,12,27)"
